<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zeng.fanda.com.viewdemo">

    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lion"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".base.BaseApplication"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name=".EntranceSpec"
            android:enabled="false"
            android:icon="@mipmap/app_icon"
            android:label="ViewDemo"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity android:name=".ui.Pratice1Activity"/>
        <activity android:name=".ui.Pratice2Activity"/>
        <activity android:name=".ui.Pratice3Activity"/>
        <activity android:name=".ui.Pratice4Activity"/>
        <activity android:name=".ui.Pratice5Activity"/>
        <activity android:name=".ui.Pratice6Activity"/>
        <activity android:name=".ui.Pratice7Activity"/>
        <activity android:name=".ui.Pratice8Activity"/>
        <activity android:name=".ui.Pratice9Activity"/>
    </application>

</manifest>